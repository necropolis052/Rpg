Function save() As String

        Dim Path As String = "U:\computer science\ELON GATED.txt"

        Dim dlol As String
        Dim dElon As String
        Dim dGate As String
        Dim dDate As String
        If dlol = "lol" Then
            File.Delete("U:\computer science\ELON GATED.txt")
        End If
        dlol = "lol"
        dElon = "ELON"
        dGate = "WAS MADE LONGER"
        dDate = "Â© James Oxley 2022"
        FileOpen(1, Path, OpenMode.Output)
        PrintLine(1, dlol)
        PrintLine(1, dElon, " ", dGate)
        PrintLine(1, "")
        For x = 0 To 7
            PrintLine(1, CStr(inv(x).exists))
        Next
        PrintLine(1, name.pclass)
        PrintLine(1, dDate)


        FileClose(1)
        'read()

    End Function
    Function read() As String


        Dim Path As String = "U:\computer science\ELON GATED.txt"
        Dim y As Boolean

        FileOpen(1, Path, OpenMode.Input)

        For x = 0 To 7
            y = LineInput(CBool(inv(x).exists))
            If y = True Then
                inv(x).exists = True
            End If
        Next


        FileClose(1)
    End Function

